{

    "manifest_version": 2,
    "name": "IRCTC Booked History Scrape",
    "description": "Scrapes your Indian Railways Booked Ticket History",
    "version": "1.0",
    "applications": {
      "gecko": {
        "strict_min_version": "54.0a1"
      }
    },
  
    "sidebar_action": {      
      "default_title" : "IRCTC Tickets Scraper",
      "default_panel": "sidebar/main.html"
    },
  
    "permissions": ["tabs", "webRequest", "<all_urls>", "activeTab"]  
  }